<script>
    var url = window.location.pathname.toLowerCase();
    var site_type = /iPad/.test(navigator.userAgent) ? "t" : /Mobile|iP(hone|od)|Android|BlackBerry|IEMobile|Silk/.test(navigator.userAgent) ? "m" : "d";
    window.criteo_q = window.criteo_q || [];

    //homepage
    if (url == "" || url == "/") {
        
        window.criteo_q.push(
            { event: "setAccount", account: 26476 },
            { event: "setHashedEmail", email: "" },
            { event: "setSiteType", type: site_type },
            { event: "viewHome" }
        );
    }

    //hitlist
    if (url.indexOf(/s/) >= 0) {

        var top3hitlist = [];

        for (var i = 0; i < 3; i++) {
            if ($("span.item-number")[i] != null) {
                top3hitlist[i] = $("span.item-number")[i].innerHTML;
            } else {
                top3hitlist[i] = '';
            }
        }

        //console.log("Criteo - Hitlist: Product1: " + top3hitlist[0] + "; Product2: " + top3hitlist[1] + "; Product3: " + top3hitlist[2] + "; Email: " + '')

        window.criteo_q.push(
            { event: "setAccount", account: 26476 },
            { event: "setHashedEmail", email: "" },
            { event: "setSiteType", type: site_type },
            { event: "viewList", item: top3hitlist }
            //{ event: "viewList", item: [top3hitlist[0], top3hitlist[1], top3hitlist[2]] }
        );
    }

    //product info
    if (url.indexOf("/product/") >= 0) {
        //var number = url.substr(url.lastIndexOf('/') + 1);
        var number = $('input.skuhelper').val().replace(/ +(?= )/g, '');

        //console.log("Criteo - Product Info: Number: " + number + "; Email: " + '')

        window.criteo_q.push(
            { event: "setAccount", account: 26476 },
            { event: "setHashedEmail", email: "" },
            { event: "setSiteType", type: site_type },
            { event: "viewItem", item: number }
        );
    }

    //shopping cart
    if (url.indexOf('/shoppingcart') >= 0) {

        var items = []; //array of cart products
        var cartItems = $(".CartProduct", $("#shoppingCart"));

        //var string = '[';
        cartItems.each(function ()  //iterate through cart products and add them to items array
        {
            if ($(this).find("span.itemNumber").text().length > 0) {
                var item = {};
                item['id'] = $(this).find("span.itemNumber").text();
                item['price'] = $(this).find("span.itemPrice").text();
                item['quantity'] = $(this).find("input.itemQty").val();
                //string += '{id: "' + item.id + '", price: "' + item.price + '", quantity: "' + item.quantity + '"},'
                items.push(item);
            }
        })

        //console.log("Criteo - Shopping Cart: Items: " + JSON.stringify(items) + "; Email: " + '')

        window.criteo_q.push(
            { event: "setAccount", account: 26476 },
            { event: "setHashedEmail", email: "" },
            { event: "setSiteType", type: site_type },
            { event: "viewBasket", item: items }
        );
    }

    //if order confirmation
    if (url.indexOf('/checkout/confirmation') >= 0) {

        var orderNumber = $('#InternetId').text();
        var orderItems = $(".order-items");

        //console.log("Criteo Internet Id: " + orderNumber)
        //console.log("Criteo Order Items: " + orderItems)

        var items = []; //array of order products
        orderItems.each(function ()  //iterate through cart products and add them to items array
        {
            if ($(this).find("input.itemNumber").val().length > 0) {
                var item = {};
                item['id'] = $(this).find("input.itemNumber").val();
                item['price'] = $(this).find("input.itemPrice").val();
                item['quantity'] = $(this).find("input.itemQty").val();
                //string += '{id: "' + item.id + '", price: "' + item.price + '", quantity: "' + item.quantity + '"},'
                items.push(item);
            }
        })

        //console.log("Criteo - Place Order: Items: " + JSON.stringify(items) + "; Order Number: " + orderNumber + "; Email: " + '' + "; Deduplication: " + '0' + "; New Customer: " + '1')

        window.criteo_q.push (
            { event: "setAccount", account: 26476 },
            { event: "setHashedEmail", email: "" },
            { event: "setSiteType", type: site_type },
            { event: "trackTransaction", id: orderNumber, deduplication: 0, new_customer: 1, item: items }
        );
    }

</script>
